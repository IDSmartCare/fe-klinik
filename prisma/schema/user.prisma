model User {
  id        Int      @id @default(autoincrement())
  name      String?  @db.VarChar(200)
  email     String   @unique @db.VarChar(200)
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  role      Role
}

enum Role {
  admin
  admisi
  perawat
  dokter
  farmasi
  gudang
}
